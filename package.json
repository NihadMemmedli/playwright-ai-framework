  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:api": "playwright test tests/api/",
    "test:ui-only": "playwright test tests/ui/",
    "test:integration": "playwright test tests/integration/",
    "test:headless": "playwright test --headed=false",
    "test:chrome": "playwright test --project=chromium",
    "test:chrome:api": "playwright test --project=chromium tests/api/",
    "test:chrome:ui": "playwright test --project=chromium tests/ui/",
    "test:chrome:integration": "playwright test --project=chromium tests/integration/",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\"",
    "report": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "clean": "rm -rf allure-results allure-report playwright-report test-results",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "debug": "playwright test --debug",
    "show-report": "npx playwright show-report"
  },
